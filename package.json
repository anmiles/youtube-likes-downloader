{
	"name": "youtube-likes-downloader",
	"version": "3.0.3",
	"description": "Downloads all liked videos",
	"keywords": [
		"youtube",
		"download",
		"likes"
	],
	"author": "Anatoliy Oblaukhov",
	"homepage": "https://gitlab.com/anmiles/youtube-likes-downloader",
	"repository": "gitlab:anmiles/youtube-likes-downloader",
	"license": "MIT",
	"engines": {
		"node": ">=12.22.1"
	},
	"main": "dist/index.js",
	"scripts": {
		"clean": "rimraf dist",
		"build": "rimraf dist && tsc",
		"lint": "eslint --ext .js,.ts .",
		"lint:fix": "eslint --fix --ext .js,.ts .",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watch",
		"test:watch:coverage": "jest --watch --coverage",
		"test:report:coverage": "nyc report --nycrc-path ./coverage.config.js -t ./coverage --report-dir ./coverage",
		"start": "node ./dist/index.js",
		"create": "node ./dist/create.js",
		"migrate": "node ./dist/migrate.js",
		"login": "node ./dist/login.js"
	},
	"dependencies": {
		"colorette": "^2.0.19",
		"execa": "^5.1.1",
		"googleapis": "^104.0.0",
		"rimraf": "^3.0.2"
	},
	"devDependencies": {
		"@types/jest": "^28.1.3",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.18.0",
		"eslint-plugin-align-assignments": "^1.1.2",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^26.5.3",
		"jest": "^28.1.2",
		"nyc": "^15.1.0",
		"ts-jest": "^28.0.5",
		"typescript": "^4.7.4"
	}
}
