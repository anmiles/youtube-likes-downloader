{
  "name": "youtube-likes-downloader",
  "version": "8.0.0",
  "description": "Download all liked videos from youtube",
  "keywords": [
    "youtube",
    "download",
    "likes"
  ],
  "author": "Anatoliy Oblaukhov",
  "homepage": "https://gitlab.com/anmiles/youtube-likes-downloader",
  "repository": "gitlab:anmiles/youtube-likes-downloader",
  "license": "MIT",
  "engines": {
    "node": ">=18.14.2"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --verbose",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm test -- --ci --coverage",
    "test:watch": "npm test -- --watch",
    "test:watch:coverage": "npm test -- --watch --coverage",
    "test:report:coverage": "nyc report --nycrc-path ./coverage.config.js -t ./coverage --report-dir ./coverage",
    "start": "node ./dist/index.js",
    "check": "node ./dist/check.js",
    "update": "node ./dist/update.js",
    "create": "node ./dist/create.js",
    "login": "node ./dist/login.js"
  },
  "dependencies": {
    "@anmiles/google-api-wrapper": "^15.3.0",
    "@anmiles/logger": "^4.0.1",
    "@anmiles/prototypes": "^5.1.1",
    "execa": "^5.1.1",
    "googleapis": "^126.0.1"
  },
  "devDependencies": {
    "@anmiles/eslint-config": "^2.0.2",
    "@anmiles/jest-extensions": "^2.0.1",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
